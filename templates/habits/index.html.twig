{% block javascripts %}

    <!-- javascript-->
        {{ parent() }}
        <script>
            $(document).ready(function(){
            $('.myButton').click(function() {
                if ($(this).attr('status')=='check'){
                    $(this).attr("status", "uncheck")
                    $(this).attr("data-bs-toggle", "0")
                    $(this).attr("data-bs-target", "0")
                    $(this).addClass("uncheck");
                    $(this).removeClass("check");
                    console.log($(this).attr('status'), $(this).attr('class'));
                }
                else{
                    $(this).attr("status", "check")
                    $(this).addClass("check");
                    $(this).removeClass("uncheck");
                    console.log($(this).attr('status'), $(this).attr('class'));
                }

                $.ajax({
                    type: 'GET',
                    url: $(this).attr('href'),
                    dataType: 'json',

                    success: function(response) {

                        $('.streak-' + response.id).text(response.streak);
                        console.log('#habit-' + response.id);

                    }

                });
            });
            });


        </script>
