<!-- body of the page-->
    <div class="box">

<!-- habits that should be completed today -->
    <div class="today-habits">
         <h1>Today</h1>

    {% if todayHabits is defined and todayHabits|length %}
    {% for habit in todayHabits %}
        <!-- habit -->
        <div class="habit" id="habit-{{ habit.id }}">
            <div class="habit-container">
                <!-- name -->
                <div>{{ habit.name }}</div>
                <!-- days -->
                <div class="days">
                    {% if (habit.monday == 1) %}
                        <div><i class="fa-solid fa-circle"></i></div>
                    {% else %}
                        <div><i class="fa-regular fa-circle"></i></div>
                    {%endif%}

                    {% if (habit.tusday == 1) %}
                        <div><i class="fa-solid fa-circle"></i></div>
                    {% else %}
                        <div><i class="fa-regular fa-circle"></i></div>
                    {%endif%}

                    {% if (habit.wednesday == 1) %}
                        <div><i class="fa-solid fa-circle"></i></div>
                    {% else %}
                        <div><i class="fa-regular fa-circle"></i></div>
                    {%endif%}

                    {% if(habit.thursday == 1) %}
                        <div><i class="fa-solid fa-circle"></i></div>
                    {% else %}
                        <div><i class="fa-regular fa-circle"></i></div>
                    {%endif%}

                    {% if (habit.friday == 1 )%}
                        <div><i class="fa-solid fa-circle"></i></div>
                    {% else %}
                        <div><i class="fa-regular fa-circle"></i></div>
                    {%endif%}

                    {% if (habit.sathurday == 1) %}
                        <div><i class="fa-solid fa-circle"></i></div>
                    {% else %}
                        <div><i class="fa-regular fa-circle"></i></div>
                    {%endif%}

                    {% if (habit.sunday == 1) %}
                        <div><i class="fa-solid fa-circle"></i></div>
                    {% else %}
                        <div><i class="fa-regular fa-circle"></i></div>
                    {%endif%}
                </div>
            </div><!-- habit-container -->
            <div class="streak-{{ habit.id }} streak">{{ habit.streak}}</div>

            <!-- modal buttons -->
            {% if ( habit.lastExecution|date("m/d/Y") == date|date("m/d/Y") ) %}
                <button habit-id="{{ habit.id }}" id="Button-{{ habit.id }}" type="button"
                        class="myButton uncheck" status="uncheck" data="{{ habit.id }}"  href="{{ url('habit_change', {id: habit.id}) }}" title="{{ 'action.view'|trans }}">
                    button
                </button>

           {% else %}
                <button habit-id="{{ habit.id }}" id="Button-{{ habit.id }}" type="button"
                        class="myButton check" status="check"  data-bs-toggle="modal" data-bs-target="#exampleModalLong-{{ habit.id }}"
                        data="{{ habit.id }}"  href="{{ url('habit_change', {id: habit.id}) }}" title="{{ 'action.view'|trans }}">
                    button
                </button>
           {% endif %}

            <!-- Modal window -->
            <div class="modal fade" id="exampleModalLong-{{ habit.id }}" tabindex="-1" role="dialog" aria-labelledby="exampleModalLongTitle" aria-hidden="true">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
