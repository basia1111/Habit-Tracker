            <div id="my-element" data-my-variable="{{registrationError }}"></div>
            <!-- left side- login form -->
            <div class="wrapper {% if  registrationError  is defined and  registrationError == 1  %} active {% endif %}">

                <!-- sign-up form -->
                <div class="form-wrapper sign-up">

                    <!-- header -->
                    <div>
                        <h1 class="h3">Register</h1>
                        <div class="signUp-link">
                            <p class="small-text">Already have an account?
                                <a href="#" class="signInBtn-link link">Sign In</a>
                            </p>
                        </div>
                    </div>

                    <!-- form -->
                    <div>
                        {{ form_start(registrationForm) }}
                        <div class="errors">
                            <!-- errors -->
                            {{ form_errors(registrationForm.email) }}
                            {{ form_errors(registrationForm.plainPassword) }}
                        </div>
                        <!-- inputs -->
                        <div class="input-wrap">
                            {{ form_widget(registrationForm.email) }}
                            <label>Email</label>
                        </div>
                        <div id="password-s" class="input-wrap ">
                            {{ form_widget(registrationForm.plainPassword) }}
                            <label>Password</label>
                        </div>
                        <div id="agree" class=" input-wrap">
                            {{ form_widget(registrationForm.agreeTerms) }}
                            <p class="small-text"> Agree to terms</p>
                        </div>

                        <!-- submit -->
                        <button  type="submit" class="button">Register</button>

                        {{ form_end(registrationForm) }}
                    </div>
                </div>

                <!-- sign-in form -->
                <div class="form-wrapper sign-in">

                    <!-- header -->
                    <div>
                        <h1 class="h3 ">Please sign in</h1>
                        <div class="signUp-link">
                            <p class="small-text"> Don't have account yet?
                                <a href="#" class="signUpBtn-link link ">Sign Up</a>
                            </p>
                        </div>
                    </div>

                    <!-- Form -->
                    <form method="post">

                        <!-- errors -->
                        {% if error %}
                            <div class="errors">
                                Invalid login or password!
                            </div>
                        {% endif %}

                        <!-- inputs -->
                        <div class="input-wrap">


                            <input id="login-name" type="email"  name="email" autocomplete="email"  required autofocus>
                            <label>Email</label>
                        </div>
                        <div id="password-r" class="input-wrap ">
                            <input type="password" name="password"  autocomplete="new-password" required>
                            <label>Passowrd</label>
                        </div>

                        <!-- Forgot password -->
                        <div>
                            <a class="small-text forgot link" href="{{ path('app_forgot_password_request')}}">Forgot password?</a>
                        </div>

                        <input type="hidden" name="_csrf_token" value="{{ csrf_token('authenticate') }}">

                        <!-- submit -->
                        <div>
                            <button class="button"  type="submit">
                                Sign in
                            </button>
                        </div>
                    </form>

                </div>  <!-- form-wrapper -->

            </div>  <!-- wrapper -->
        </div>  <!-- inner-box -->
