<h2>Ctrate Post</h2>

<form id="form-create-post" class="create-form" action="{{ path('post_create')}}" enctype="multipart/form-data" method="post">
    {{ form_row(form.title) }}
    {{ form_row(form.cover) }}


    {{ form_row(form.content) }}
    {{ form_row(form._token) }}

    <button type="submit">Create</button>
</form>